{
  "name": "@shadow-sniper/contract",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "compact compile src/shadow_sniper.compact src/managed/shadow_sniper",
    "build": "rm -rf dist && tsc --project tsconfig.json && cp -Rf ./src/managed ./dist/managed && cp ./src/shadow_sniper.compact ./dist",
    "clean": "rm -rf dist"
  }
}
